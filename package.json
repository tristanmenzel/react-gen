{
  "name": "@tristanwritescode/react-gen",
  "version": "1.1.0",
  "repository": {
    "type": "GIT",
    "url": "https://github.com/tristanmenzel/react-gen"
  },
  "description": "Generates code for react apps",
  "main": "dist/index.js",
  "bin": {
    "@tristanwritescode/react-gen": "bin/react-gen.js",
    "react-gen": "bin/react-gen.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "dev": "cd wd && ts-node dev.ts",
    "lint:fix": "eslint src/**/*.ts --fix --ignore-path .eslintignore",
    "lint:check": "eslint src/**/*.ts --ignore-path .eslintignore",
    "test": "jest",
    "format": "prettier --write .",
    "lint": "eslint \"src/**/*.ts\""
  },
  "author": "Tristan Menzel",
  "license": "MIT",
  "devDependencies": {
    "@makerx/eslint-config": "^3.0.0",
    "@makerx/prettier-config": "2.0.0",
    "@makerx/ts-config": "1.0.1",
    "@types/jest": "^29.2.2",
    "@types/mustache": "^4.2.1",
    "@types/node": "^18.11.9",
    "eslint": "8.22.0",
    "jest": "^29.2.2",
    "npm-run-all": "^4.1.5",
    "prettier": "2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.0"
  },
  "dependencies": {
    "ajv": "^8.11.0",
    "arg": "^5.0.2",
    "change-case": "^4.1.2",
    "colors": "^1.4.0",
    "esm": "^3.2.25",
    "mustache": "^4.2.0",
    "tslib": "^2.4.1"
  }
}
